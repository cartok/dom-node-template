{
  "name": "dom-node-template",
  "version": "2.1.8",
  "main": "./dist/NodeTemplate.js",
  "description": "create usefull html or svg element wrapper object with es6 template string.",
  "keywords": [
    "dom",
    "parser",
    "node",
    "element",
    "template",
    "jsx",
    "svg",
    "template",
    "dom parser",
    "template string",
    "string template",
    "template literal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cartok/dom-node-template"
  },
  "author": "Dennis Weiersh√§user <dennis.weiershaeuser@informatik.hs-fulda.de>",
  "license": "Apache-2.0",
  "scripts": {
    "babelrc:es5": "cp ./.babelrc.es5 ./.babelrc",
    "babelrc:es6": "cp ./.babelrc.es6 ./.babelrc",
    "babel": "npm install && babel --out-dir ./dist ./src",
    "build:es5": "npm run babelrc:es5 && npm run babel",
    "build:es6": "npm run babelrc:es6 && npm run babel",
    "build:webpack": "npm run babelrc:es5 && webpack --mode=production",
    "build:all": "npm run build:webpack && npm run build:es6",
    "test": "npm run build:es5 && mocha --reporter mochawesome --require babel-core/register --require jsdom-global/register && chromium ./mochawesome-report/mochawesome.html",
    "test:monitor": "nodemon -x \"npm run test\"",
    "prepublishOnly": "npm run test:all && npm run build:all"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.x.x",
    "eslint": "^4.x.x",
    "eslint-plugin-flowtype": "^2.48.x",
    "flow-bin": "^0.73.0",
    "http-server": "^0.10.x",
    "jquery": "^3.x.x",
    "jsdom": "^11.11.x",
    "jsdom-global": "^3.0.x",
    "mocha": "^5.2.x",
    "mocha-simple-html-reporter": "^1.1.0",
    "mochawesome": "^3.0.3",
    "nodemon": "^1.17.x",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {
    "@cartok/html-tag-names": "^1.1.4",
    "@cartok/svg-tag-names": "^1.1.2"
  },
  "nodemonConfig": {
    "watch": [
      "test/**/*",
      "src/**/*"
    ],
    "ext": "js",
    "delay": 500,
    "verbose": true
  }
}
