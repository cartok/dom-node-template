{
  "name": "dom-node-template-es6",
  "version": "1.2.0",
  "description": "create usefull html node wrapper object with es6 template string.",
  "keywords": [
    "dom",
    "node",
    "template",
    "jsx",
    "dom parser",
    "template string",
    "string template",
    "template literal"
  ],
  "main": "./build/NodeTemplate.js",
  "scripts": {
    "sound:plock": "play -q -v 0.2 $(find /usr/share/sounds/ -type f -name *device-added* | head -n 1)",
    "sound:microwave": "play -q -v 0.1 $(find /usr/share/sounds/ -type f -name *complete* | head -n 1)",
    "sound:trash": "play -q -v 0.15 $(find /usr/share/sounds/ -type f -name *trash*  | head -n 1)",
    "clean:js": "rm $(find ./build/ -type f -regex '.*\\.[jt]sx?') && npm run --silent sound:trash",
    "clean:dir": "rm -rf ./build && mkdir build && npm run --silent sound:trash",
    "babelrc:build": "cp ./.babelrc.build ./.babelrc",
    "babelrc:mocha": "cp ./.babelrc.mocha ./.babelrc",
    "mocha": "npm run --silent babelrc:mocha && ./node_modules/.bin/mocha --compilers js:babel-core/register --require jsdom-global/register",
    "babel": "./node_modules/.bin/babel --out-dir ./build ./src",
    "--------------------------------------------------------------------------------------------------------------------------------------------": "",
    "build": "clear; npm run --silent clean:dir && npm run --silent babelrc:build && npm run --silent babel && npm run --silent sound:microwave",
    "test": "clear; npm run --silent babelrc:mocha && npm run --silent babel && npm run --silent mocha && npm run --silent sound:plock",
    "test:monitor": "nodemon -x \"npm run --silent test\"",
    "prepublishOnly": "npm run --silent test && npm run --silent build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cartok/dom-node-template"
  },
  "author": "Dennis Weiersh√§user <dennis.weiershaeuser@l3p-cv.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "*",
    "babel-eslint": "*",
    "babel-plugin-transform-flow-strip-types": "*",
    "babel-preset-env": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-plugin-flowtype": "*",
    "flow-bin": "*",
    "jquery": "3.x.x",
    "jsdom": "*",
    "jsdom-global": "*",
    "mocha": "*",
    "nodemon": "*"
  },
  "dependencies": {
    "html-element-attributes": "^1.3.0",
    "html-tag-names": "^1.1.2",
    "svg-element-attributes": "^1.2.0",
    "svg-tag-names": "^1.1.1"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "built",
      "dist",
      "bin",
      "node_modules"
    ]
  }
}
