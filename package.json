{
  "name": "dom-node-template",
  "version": "2.1.9",
  "main": "./dist/NodeTemplate.js",
  "description": "usefull and performant way to create html/svg elements",
  "keywords": [
    "dom",
    "parser",
    "node",
    "element",
    "template",
    "jsx",
    "svg",
    "template",
    "dom parser",
    "template string",
    "string template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cartok/dom-node-template"
  },
  "author": "Dennis Weiersh√§user <dennis.weiershaeuser@informatik.hs-fulda.de>",
  "license": "MIT License",
  "scripts": {
    "babelrc:es5": "cp ./babel.config.es5.js ./babel.config.js",
    "babelrc:es6": "cp ./babel.config.es6.js ./babel.config.js",
    "babel": "npm install && babel --out-dir ./dist ./src",
    "build:es5": "npm run babelrc:es5 && npm run babel",
    "build:es6": "npm run babelrc:es6 && npm run babel",
    "build:webpack": "npm run babelrc:es5 && webpack --mode=production",
    "build:all": "npm run build:webpack && npm run build:es6",
    "test": "npm run build:es5 && mocha -R mochawesome -r @babel/register -r jsdom-global/register && chromium ./mochawesome-report/mochawesome.html",
    "test:monitor": "nodemon -x \"npm run test\"",
    "prepublishOnly": "npm run test && npm run build:all"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-parameters": "7.1.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "flow-bin": "^0.81.0",
    "http-server": "^0.10.0",
    "jquery": "^3.3.1",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "nodemon": "^1.18.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "@cartok/html-tag-names": "^1.1.4",
    "@cartok/svg-tag-names": "^1.1.2"
  },
  "nodemonConfig": {
    "watch": [
      "test/**/*",
      "src/**/*"
    ],
    "ext": "js",
    "delay": 500,
    "verbose": true
  }
}
